{
  "name": "url-maliciousness-detector",
  "description": "AI-powered URL classification system with multi-algorithm detection",
  "keywords": ["machine-learning", "security", "url-detection", "streamlit", "python"],
  "website": "https://url-detector.streamlit.app",
  "repository": "https://github.com/yourusername/url-detection-model",
  "logo": "https://raw.githubusercontent.com/yourusername/url-detection-model/main/logo.png",
  "success_url": "/",
  "stack": "heroku-22",
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:mini",
      "options": {
        "version": "14"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "STREAMLIT_SERVER_PORT": {
      "description": "Port for Streamlit server",
      "value": "8501"
    },
    "STREAMLIT_SERVER_ADDRESS": {
      "description": "Address for Streamlit server", 
      "value": "0.0.0.0"
    },
    "PYTHONPATH": {
      "description": "Python path for modules",
      "value": "/app"
    }
  },
  "scripts": {
    "postdeploy": "echo 'Deployment complete!'"
  }
}